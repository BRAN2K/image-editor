{
  "output": {
    "filePath": "repomix-output.md",
    "style": "markdown",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": false
  },
  "include": ["src/**/*.tsx", "src/**/*.ts"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": ["src/themes/*", "src/icons/*"]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
